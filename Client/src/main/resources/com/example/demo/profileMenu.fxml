<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.demo.view.ProfileControllerFx"
            maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="800.0" prefWidth="1280.0" fx:id="pane">
    <ImageView fx:id="background" fitWidth="1280.0">
        <Image url="@assets/backgrounds/profileMenuBackground.png"/>
    </ImageView>
    <Button fx:id="profile" layoutX="100" layoutY="20.0" mnemonicParsing="false"
            prefHeight="200.0" prefWidth="200.0"/>
    <PasswordField fx:id="newPassword" layoutX="550" layoutY="100.0" promptText="new Password"/>
    <PasswordField fx:id="oldPassword" layoutX="550" layoutY="160.0" promptText="current Password"/>
    <TextField fx:id="nickname" layoutX="550" layoutY="220.0" promptText="Nickname"/>
    <Button fx:id="confirm" onAction="#changeUserData" layoutX="550" layoutY="340.0" mnemonicParsing="false"
            prefHeight="25.0" prefWidth="100.0" text="confirm"/>
    <Button fx:id="exit" onMouseClicked="#exit" layoutX="550" layoutY="380.0" mnemonicParsing="false"
            prefHeight="25.0" prefWidth="100.0" text="logout"/>
    <Button fx:id="back" onMouseClicked="#back" layoutX="1400" layoutY="10.0" mnemonicParsing="false"
            prefHeight="25.0" prefWidth="100.0" text="back"/>
    <ScrollPane fx:id="gameInvitationScrollBar" />

    <TextField fx:id="sendRequestTextField"/>
    <Button fx:id="sendRequestButton"/>





</AnchorPane>
